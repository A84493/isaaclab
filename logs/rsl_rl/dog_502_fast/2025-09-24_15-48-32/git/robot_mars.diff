+def apply_saved_gravity(env_cfg, log_dir: str) -> None:
+    """Restore gravity parameters from a logged run directory into the environment config."""
+
+    if not log_dir or not hasattr(env_cfg, "gravity_mode"):
+        return
+
+    params_dir = os.path.join(log_dir, "params")
+    candidates = [
+        os.path.join(params_dir, "gravity.yaml"),
+        os.path.join(params_dir, "env.yaml"),
+    ]
+
+    config_data = None
+    for path in candidates:
+        if os.path.exists(path):
+            try:
+                config_data = OmegaConf.to_container(OmegaConf.load(path), resolve=True)
+                break
+            except Exception:  # pragma: no cover - defensive against malformed files
+                continue
+
+    if not isinstance(config_data, dict):
+        return
+
+    mode = config_data.get("mode") or config_data.get("gravity_mode")
+    if mode is None:
+        return
+
+    def _as_tuple(value):
+        if value is None:
+            return None
+        if isinstance(value, dict):
+            value = list(value.values())
+        if not isinstance(value, (list, tuple)):
+            value = [value]
+        return tuple(float(v) for v in value)
+
+    env_cfg.gravity_mode = mode
+
+    custom = config_data.get("custom_gravity")
+    custom_tuple = _as_tuple(custom)
+
+    if mode == "other":
+        if custom_tuple is None:
+            vector_from_file = _as_tuple(config_data.get("vector"))
+            if vector_from_file is not None:
+                custom_tuple = vector_from_file
+        if custom_tuple is not None:
+            env_cfg.custom_gravity = custom_tuple
+    elif hasattr(env_cfg, "custom_gravity"):
+        env_cfg.custom_gravity = None
+
+    vector = _as_tuple(config_data.get("vector"))
+    if vector is not None and hasattr(env_cfg, "sim") and hasattr(env_cfg.sim, "gravity"):
+        env_cfg.sim.gravity = tuple(vector)
+
+    if hasattr(env_cfg, "_apply_gravity_profile"):
+        env_cfg._apply_gravity_profile()
+
+    applied_vector = getattr(getattr(env_cfg, "sim", None), "gravity", None)
+    if applied_vector is not None:
+        print(f"[INFO] Restored gravity profile '{env_cfg.gravity_mode}' with vector {applied_vector}.")
